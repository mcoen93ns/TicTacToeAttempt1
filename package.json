{
  "name": "tictactoeattempt1",
  "version": "1.0.0",
  "description": "Kata TicTacToeAttempt1",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "commitlint": "^17.2.0",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.6.4",
    "jest-cucumber": "^3.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose --coverage",
    "watch": "jest --watchAll --color --coverage --verbose",
    "_lint": "eslint --config ./.eslintrc.json --ext js --ext jsx --ext tsx --ext ts",
    "lint": "npm run _lint -- ./src/ ./test/",
    "lint:fix": "npm run _lint -- --fix ./src/ ./test/",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,scss,md,json}' --config ./.prettierrc",
    "prepare-githooks": "husky install",
    "start": "node ./dist/api.js",
    "docker_build": "docker build -t mathijs/tictactoe:local .",
    "docker_compose_refresh": "docker-compose --project-name tictactoestack down && npm run docker_build && npm run dive && docker-compose --project-name tictactoestack up -d && newman run smoketest.postman_collection.json",
    "docker_compose_logs": "docker-compose --project-name tictactoestack logs -f",
    "docker_compose_destroy": "docker-compose --project-name tictactoestack down",
    "dive": "docker run --rm -it -v /var/run/docker.sock:/var/run/docker.sock -e CI=true wagoodman/dive:latest mathijs/tictactoe:local"
  },
  "author": "",
  "license": "ISC"
}
